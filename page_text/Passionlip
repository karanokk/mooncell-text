\n        changeGraph(i - 1);\n        changeSelect(i - 1);\n      }\n      break;\n    }\n  }\n});\n\n$(\".graphpicker-next\").click(function () {\n  for (i = 0; i < count; i++) {\n    if ($(\".graphpicker-graph-\" + i).css(\"display\") == \"block\") {\n      if (i == (count - 1)) {\n        changeGraph(0);\n        changeSelect(0);\n      } else {\n        changeGraph(i + 1);\n        changeSelect(i + 1);\n      }\n      break;\n    }\n  }\n});\n\nfunction changeGraph(p) {\n  for (q = 0; q < count; q++) {\n    if (q == p) {\n      $(\".graphpicker-graph-\" + q).css(\"display\", \"block\");\n      $(\".svtcardname-\" + q).css(\"display\", \"inline\");\n    } else {\n      $(\".graphpicker-graph-\" + q).css(\"display\", \"none\");\n      $(\".svtcardname-\" + q).css(\"display\", \"none\");\n    }\n  }\n}\n\nfunction changeSelect(p) {\n  $(\"select.graphpicker-select option\").prop(\"selected\", false);\n  $(\".graphpicker-option-\" + p).prop(\"selected\", true);\n  $(\".graphpicker-option-\" + p).change();\n}\n</script>\n<!--seostart--></p><p id=\"wikiseoe0af\"><!--\nWikiSEO:title;TW9vbmNlbGwgLSDnjqnlrrboh6rnlLHmnoTnrZHnmoRGR0/kuK3mlodXaWtpCg==\nWikiSEO:title_mode;YXBwZW5k\nWikiSEO:keywords;bGlwLOW4leajruiOieaZrizpg4Hph5Hpppks54Ot5oOF6L+35ZSHCg==\nWikiSEO:description;TW9vbmNlbGzmmK/kuIDkuKrpnZ7nm4jliKnmgKfotKjnmoRGR0/otYTmlplXaWtp572R56uZCg==\n--></p><!--seoend-->\n<h2><span id=\"\u5b9d\u5177\"></span><span class=\"mw-headline\" id=\".E5.AE.9D.E5.85.B7\">\u5b9d\u5177</span><span class=\"mw-editsection\"><span class=\"mw-editsection-bracket\">[</span><a href=\"/index.php?title=Passionlip&amp;action=edit&amp;section=2\" title=\"\u7f16\u8f91\u5c0f\u8282\uff1a\u5b9d\u5177\">\u7f16\u8f91</a><span class=\"mw-editsection-bracket\">]</span></span></h2>\n<div class=\"nomobile cbox-autonarrow\" style=\"margin-bottom:12px; margin-top:12px;display:flex; flex-direction: row; flex-wrap: nowrap; background-color: #F9FFEA; width:825px; max-width:100%; border-radius:5px; box-shadow: